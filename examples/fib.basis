function fib(n)
  if n == 0
    return 0
  if n <= 2
    return 1
  a = 1
  b = 1
  for (i = 2; i < n; i++)
    c = b
    b = a
    a = a + c
  return a

function fib_rec(n)
  if n == 0
    return 0
  if n == 1
    return 1
  return fib_rec(n - 1) + fib_rec(n - 2)

function fib_tail_helper(n, a, b)
  if n == 0
    return a
  if n == 1
    return b
  return fib_tail_helper(n - 1, b, a + b)

function fib_tail(n)
  return fib_tail_helper(n, 0, 1)

fib_tail(6)
